@prefix ex: <http://example.org/europe#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

ex:Country a rdfs:Class .

ex:countryName       a rdf:Property ;
    rdfs:domain ex:Country ;
    rdfs:range  xsd:string .

ex:gdpPerCapitaUSD  a rdf:Property ;
    rdfs:label "PIB per cápita (US$ actuales, aprox. 2024)"@es ;
    rdfs:domain ex:Country ;
    rdfs:range  xsd:decimal .

ex:population        a rdf:Property ;
    rdfs:label "Población total reciente"@es ;
    rdfs:domain ex:Country ;
    rdfs:range  xsd:integer .
    
