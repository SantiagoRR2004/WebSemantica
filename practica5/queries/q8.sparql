PREFIX ex: <http://example.org/europe#>

SELECT ?country ?gdp
WHERE {
    {
        SELECT (AVG(?g) AS ?avgGdp)
        WHERE {
            ?c a ex:Country ;
            ex:gdpPerCapitaUSD ?g.
        }
    }
    ?country a ex:Country ;
    ex:gdpPerCapitaUSD ?gdp.
    FILTER(?gdp > ?avgGdp)
}
ORDER BY DESC(?gdp)
