@base <https://github.com/SantiagoRR2004/WebSemantica/tree/main/renfe/santaClaus.ttl> .

@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://ex.org/movie#> .
@prefix wikidata: <http://www.wikidata.org/entity/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .


<#yo> rdf:type foaf:Person;
	foaf:name [
        rdf:type rdf:Bag;
        rdf:_1 "Santa Claus";
        rdf:_2 "Saint Nicholas";
        rdf:_3 "Saint Nick";
        rdf:_4 "Father Christmas";
        rdf:_5 "Kris Kringle";
        rdf:_6 "Santa"
    ];
	foaf:firstName "Santa";
	foaf:surname "Claus";
	foaf:birthday "15-03";
	foaf:age 1755;
	foaf:gender "male"@en;
	foaf:homepage <https://www.northpole.com/>;
	foaf:based_near <https://www.geonames.org/6269203/north-pole.html>;
	foaf:interest wikidata:Q184303 , wikidata:Q13266 , wikidata:Q19809;
	ex:likesMovie wikidata:Q714721 , wikidata:Q497850 , wikidata:Q3281244.
	
<> a foaf:Document ;
	foaf:name "Santa's profile"@en;
	foaf:primaryTopic <#yo> .

INSERT {
  <#yo> foaf:knows ?person .
}
WHERE {
  ?person a foaf:Person .
}
